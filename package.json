{
  "name": "laon-navi-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npx nodemon --watch './**/*.ts' --exec npx ts-node app.ts ",
    "start": "npx pm2 start app.ts --name=nav-server -o ./public/logs/out.log -e ./public/logs/err.log",
    "stop": "npx pm2 delete nav-server",
    "restart": "npx pm2 reload nav-server"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/request": "^2.48.8",
    "@types/request-promise": "^4.1.48",
    "@types/sequelize": "^4.28.11",
    "axios": "^0.26.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.0",
    "firebase-admin": "^10.1.0",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "pm2": "^5.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
